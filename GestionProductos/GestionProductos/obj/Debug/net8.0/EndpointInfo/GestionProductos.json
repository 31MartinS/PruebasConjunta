  "components": {
    "schemas": {
      "Categoria": {
        "required": [
          "descripcion",
          "nombre"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "maxLength": 50,
            "minLength": 0,
            "pattern": "^[A-Za-z\\s]+$",
            "type": "string"
          },
          "descripcion": {
            "maxLength": 50,
            "minLength": 0,
            "pattern": "^[A-Za-z\\s]+$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Clientes": {
        "required": [
          "apellido",
          "email",
          "nombre",
          "telefono"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "maxLength": 50,
            "minLength": 0,
            "pattern": "^[A-Za-z\\s]+$",
            "type": "string"
          },
          "apellido": {
            "maxLength": 50,
            "minLength": 0,
            "pattern": "^[A-Za-z\\s]+$",
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "telefono": {
            "minLength": 1,
            "pattern": "^\\d{10}$",
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Producto": {
        "required": [
          "descripcion",
          "nombre",
          "precio",
          "stock"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "categoriaId": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "maxLength": 50,
            "minLength": 0,
            "pattern": "^[A-Za-z\\s]+$",
            "type": "string"
          },
          "descripcion": {
            "maxLength": 50,
            "minLength": 0,
            "pattern": "^[A-Za-z\\s]+$",
            "type": "string"
          },
          "precio": {
            "minimum": 0,01,
            "pattern": "^\\d+([.,]\\d+)?$",
            "type": "number",
            "format": "double"
          },
          "stock": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Ventas": {
        "required": [
          "cantidad",
          "fecha_venta_total",
          "total"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "productoId": {
            "type": "integer",
            "format": "int32"
          },
          "cantidad": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "fecha_venta_total": {
            "type": "string",
            "format": "date-time"
          },
          "total": {
            "minimum": 0,01,
            "pattern": "^\\d+([.,]\\d+)?$",
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
    }
